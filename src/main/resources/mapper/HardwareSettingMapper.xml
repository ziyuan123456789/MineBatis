<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.Mapper.HardwareSettingMapper">
    <select id="getOneHardware" resultType="org.example.JavaBeans.HardwareSetting">
        select * from hardwaresetting where HardwareSettingId=#{HardwareSettingId} limit 1
    </select>
</mapper>